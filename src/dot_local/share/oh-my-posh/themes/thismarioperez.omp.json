{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#d6deeb",
                    "properties": {
                        "cache_duration": "none",
                        "windows": "\ue62a"
                    },
                    "style": "plain",
                    "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
                    "type": "os"
                },
                {
                    "foreground": "#c792ea",
                    "properties": {
                        "cache_duration": "none",
                        "display_host": false
                    },
                    "style": "plain",
                    "template": "{{if .Root}} <#EF5350>\uf0e7 {{.UserName}}</> {{else}} {{.UserName}}{{end}}",
                    "type": "session"
                },
                {
                    "background": "#0b2942",
                    "foreground": "#7fdbca",
                    "leading_diamond": " \ue0b6",
                    "properties": {
                        "style": "mixed"
                    },
                    "style": "diamond",
                    "template": "\ue5ff {{ .Path }}",
                    "trailing_diamond": "\ue0b4",
                    "type": "path"
                },
                {
                    "background": "#0b2942",
                    "foreground": "#82AAFF",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFCA28{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EF5350{{ end }}",
                        "{{ if gt .Ahead 0 }}#7e57c2{{ end }}",
                        "{{ if gt .Behind 0 }}#7e57c2{{ end }}"
                    ],
                    "leading_diamond": " \ue0b6",
                    "properties": {
                        "branch_template": "{{ trunc 25 .Branch }}",
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "diamond",
                    "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "trailing_diamond": "\ue0b4",
                    "type": "git"
                },
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#575656",
                    "properties": {
                        "threshold": 500,
                        "style": "austin",
                        "always_enabled": true
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "type": "node",
                    "style": "plain",
                    "foreground": "#339933",
                    "properties": {
                        "display_mode": "files",
                        "fetch_package_manager": true,
                        "fetch_version": true,
                        "npm_icon": "<#e5272d>\ue616 npm</> ",
                        "yarn_icon": "<#37aee5>\ue6a7 yarn</> ",
                        "pnpm_icon": "<#e5a100>\udb80\udec1 pnpm</> "
                    },
                    "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }}\ue718 {{ .Full }}"
                },
                {
                    "foreground": "#4063D8",
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \ue370 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "crystal"
                },
                {
                    "foreground": "#CC342D",
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "ruby"
                },
                {
                    "foreground": "#3776AB",
                    "properties": {
                        "display_mode": "context",
                        "fetch_virtual_env": false
                    },
                    "style": "plain",
                    "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "python"
                },
                {
                    "foreground": "#000000",
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "rust"
                },
                {
                    "type": "go",
                    "style": "plain",
                    "foreground": "#00ADD8",
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": true
                    },
                    "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
                },
                {
                    "type": "shell",
                    "style": "plain",
                    "foreground": "#bec5d4",
                    "template": " \uf489 {{ .Name }}"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground_templates": [
                        "{{ if eq .Code 0 }}#22DA6E{{ end }}",
                        "{{ if gt .Code 0 }}#EF5350{{ end }}"
                    ],
                    "style": "plain",
                    "template": "\u279c ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "tooltips_action": "replace",
    "tooltips": [
        {
            "type": "kubectl",
            "tips": ["kubectl"],
            "style": "plain",
            "foreground": "#326ce5",
            "template": " \uf308 {{ trunc 20 .Context }}{{ if .Namespace }}:{{ .Namespace }}{{ end }}"
        },
        {
            "type": "aws",
            "tips": ["aws"],
            "style": "plain",
            "foreground": "#FF9900",
            "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} "
        },
        {
            "type": "gcp",
            "tips": ["gcloud"],
            "style": "plain",
            "foreground": "#4285f4",
            "template": " \udb84\uddf6 {{ if .Project }}{{ .Project }}{{ end }} "
        },
        {
            "type": "command",
            "tips": ["nats"],
            "style": "plain",
            "foreground": "#0CA0D8",
            "properties": {
                "command": "nats context info 2>&1 | head -1 | awk -F'\"' '{print $2}'",
                "shell": "bash"
            },
            "template": " {{ if ne .OS \"windows\" }} \uf0ec {{ .Output }}{{ end }}"
        }
    ],
    "version": 3
}
